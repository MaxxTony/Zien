{
  "name": "zien",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "clean": "expo start -c",
    "doctor": "expo doctor",
    "info": "expo diagnostics",
    "ios:build": "EXPO_NO_CAPABILITY_SYNC=1 eas build -p ios --profile production ",
    "ios:submit": "eas submit -p ios --profile production",
    "ios:release": "EXPO_NO_CAPABILITY_SYNC=1 eas build -p ios --profile production --auto-submit",
    "android:build": "eas build -p android --profile production",
    "android:submit": "eas submit -p android --profile production",
    "android:release": "eas build -p android --profile production --auto-submit",
    "release": "npm run ios:release && npm run android:release",
    "ota:prod": "eas update --branch production --message \"OTA production update\"",
    "ota:preview": "eas update --branch preview --message \"OTA preview update\"",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
